<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.version.sz_app1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/car"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.version.sz_app1.WelcomActivity"
            android:label="@string/app_name"  android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.version.sz_app1.SelectActivity"
            android:screenOrientation="portrait"  android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.version.sz_app1.RouteActivity"
            android:label="@string/title_activity_route"  android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.version.sz_app1.BusActivity"
            android:label="@string/title_activity_bus"  android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.version.sz_app1.ListrouteActivity"
            android:label="@string/title_activity_listroute"  android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.version.sz_app1.BuslistnActivity"
            android:label="@string/title_activity_buslistn"  android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.version.sz_app1.SQLActivity"
            android:label="@string/title_activity_sql"  android:launchMode="singleTask">
        </activity>
    </application>

</manifest>